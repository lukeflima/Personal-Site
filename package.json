{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "next lint",
    "dev": "next dev",
    "prebuild": "next lint",
    "build": "npm run installQoiViewer && next build",
    "start": "next start",
    "getQoiViewer": "curl -L https://github.com/lukeflima/qoi-viewer/files/7964742/qoi-viewer.zip -o ./projects/qoi-viewer.zip",
    "prebuildQoiViewer": "npm run getQoiViewer",
    "buildQoiViewer": "unzip ./projects/qoi-viewer.zip -d ./projects/ && rm -v ./projects/qoi-viewer.zip",
    "preinstallQoiViewer": "npm run buildQoiViewer",
    "installQoiViewer": "npm install ./projects/qoi-viewer"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "next": "^12.0.9",
    "nextjs-redirect": "^5.0.2",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-drag-drop-files": "^2.2.2",
    "react-spring": "^9.3.2",
    "react-text-transition": "^1.3.0",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7"
  }
}
